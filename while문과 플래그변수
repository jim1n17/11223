package mian;

public class Main {

	public static void main(String[] args) {
		System.out.println(Math.random());
		int a = 0;
		while(a<=10) {		
			System.out.println("a는 "+a+"입니다.");
			a++;
		}
		
	int b = 0;
	while(b<=10) {		
		System.out.println("*");
		b++;
	}
	
	b = 0;
	while(b<=10) {		
		System.out.print("*");
		b++;
	}
	
	

	}

}
