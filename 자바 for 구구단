import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		
     Scanner sc = new Scanner(System.in);		
	 System.out.println("몇단 보고 싶은데?");
      
      int 몇단 = sc.nextInt();
      
      System.out.println("몇단까지 보고 싶은데?");
      
      int n = sc.nextInt();
      
      for (int i = 1; i <= n; i++) {
    	  System.out.println(몇단+" * "+i +" = "+(몇단*i));
      }   	  
      
	}

}
